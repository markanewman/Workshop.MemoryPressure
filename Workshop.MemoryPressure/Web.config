<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1"/>
  </system.web>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Arr-Disable-Session-Affinity" value="true" />
      </customHeaders>
    </httpProtocol>
    <monitoring>
      <triggers>
        <memory privateBytesInKB="210000" />
      </triggers>
      <actions value="Recycle" />
    </monitoring>
  </system.webServer>
</configuration>